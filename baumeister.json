{
  "useHandlebars": true,
  "purifyCSS": {
    "usePurifyCSS": false,
    "whitelist": [
      "*navbar*",
      "*modal*",
      "*dropdown*",
      "*carousel*",
      "*tooltip*",
      "open",
      "fade",
      "collapse",
      "collapsing",
      "in"
    ]
  },
  "generateBanners": false,
  "cacheBusting": true,
  "vendor": {
    "bundleCSS": [
      "lightbox2/dist/css/lightbox.css"
    ],
    "includeStaticFiles": [
      "lightbox2/dist/images/**"
    ]
  },
  "webpack": {
    "DefinePlugin": {
      "development": {
        "PRODUCTION": false
      },
      "production": {
        "PRODUCTION": true
      }
    },
    "ProvidePlugin": {
      "$": "jquery",
      "jQuery": "jquery"
    }
  }
}
